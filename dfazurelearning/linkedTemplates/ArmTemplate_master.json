{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dfazurelearning"
		},
		"AzureBlobStorageConn_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageConn'"
		},
		"AzureSqlDatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase'"
		},
		"AzureDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage'"
		},
		"AzureSynapseAnalytics_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSynapseAnalytics'"
		},
		"DataLakeStaging_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'DataLakeStaging'"
		},
		"AzureSqlAdventureWorks_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlAdventureWorks'"
		},
		"Trigger_properties_dpIntro_parameters_fileName": {
			"type": "string",
			"defaultValue": "cars.csv"
		},
		"Trigger_properties_dpIntro_parameters_tableName": {
			"type": "string",
			"defaultValue": "Cars"
		},
		"AzureDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mvdatalakestoragegen2.dfs.core.windows.net"
		},
		"DataLakeStaging_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mvdatalakestoragegen2.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dfazurelearning_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorageConn_connectionString": {
						"value": "[parameters('AzureBlobStorageConn_connectionString')]"
					},
					"AzureSqlDatabase_connectionString": {
						"value": "[parameters('AzureSqlDatabase_connectionString')]"
					},
					"AzureDataLakeStorage_accountKey": {
						"value": "[parameters('AzureDataLakeStorage_accountKey')]"
					},
					"AzureSynapseAnalytics_connectionString": {
						"value": "[parameters('AzureSynapseAnalytics_connectionString')]"
					},
					"DataLakeStaging_accountKey": {
						"value": "[parameters('DataLakeStaging_accountKey')]"
					},
					"AzureSqlAdventureWorks_connectionString": {
						"value": "[parameters('AzureSqlAdventureWorks_connectionString')]"
					},
					"AzureDataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage_properties_typeProperties_url')]"
					},
					"DataLakeStaging_properties_typeProperties_url": {
						"value": "[parameters('DataLakeStaging_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "dfazurelearning_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Trigger_properties_dpIntro_parameters_fileName": {
						"value": "[parameters('Trigger_properties_dpIntro_parameters_fileName')]"
					},
					"Trigger_properties_dpIntro_parameters_tableName": {
						"value": "[parameters('Trigger_properties_dpIntro_parameters_tableName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'dfazurelearning_ArmTemplate_0')]"
			]
		}
	]
}